{
    "name": "yii2tech/install",
    "description": "Project installation support extension for the Yii2 framework",
    "keywords": ["yii2", "install", "init", "local file"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii2tech/install/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/yii2tech/install/wiki",
        "source": "https://github.com/yii2tech/install"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "suggest": {
        "yii2tech/crontab": "you need this package, if you wish to setup crontab during project installation"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii2tech\\install\\": ""}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
